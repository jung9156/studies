	"ccbaMnm2": "洛東江 下流 철새 渡來地",
	"ccbaCtcdNm": "부산",
	"ccsiName": "부산전역",
	"ccbaAdmin": "\n               부산광역시\n\t\t",
	"ccbaKdcd": "16",
	"ccbaCtcd": "21",
	"ccbaAsno": "01790000",
