	"ccbaMnm2": "鎭安 銀水寺 청실배나무",
	"ccbaCtcdNm": "전북",
	"ccsiName": "진안군",
	"ccbaAdmin": "\n               진안군(진안군수)\n\t\t",
	"ccbaKdcd": "16",
	"ccbaCtcd": "35",
	"ccbaAsno": "03860000",
